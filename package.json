{
  "name": "node-red-contrib-mqtt-json",
  "version": "1.0.0",
  "description": "Node-RED MQTT input node that parses JSON payloads",
  "main": "none",
  "keywords": [
    "node-red",
    "mqtt",
    "json",
    "parse"
  ],
  "scripts": {
    "test": "camo-purge; xo",
    "lintfix": "xo --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hobbyquaker/node-red-contrib-mqtt-json.git"
  },
  "author": "Sebastian Raff <hobbyquaker@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hobbyquaker/node-red-contrib-mqtt-json/issues"
  },
  "homepage": "https://github.com/hobbyquaker/node-red-contrib-mqtt-json#readme",
  "node-red": {
    "nodes": {
      "mqtt-json": "nodes/mqtt-json.js"
    }
  },
  "dependencies": {
    "is-utf8": "^0.2.1",
    "obj-ease": "^1.0.1"
  },
  "devDependencies": {
    "camo-purge": "^1.0.2",
    "eslint-plugin-html": "^4.0.3",
    "xo": "^0.20.3"
  },
  "xo": {
    "space": 4,
    "plugin": "html",
    "global": [
      "$",
      "RED"
    ],
    "extensions": [
      "js",
      "html"
    ]
  }
}
